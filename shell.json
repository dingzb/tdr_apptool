{
  "info": {
    "network": "uci show tdripcfg | grep tdripcfg | grep -v data",
    "version": "uci show tdripcfg | grep ':' | grep -v tdripcfg | grep -v 'usb list info' | grep -v 'Bus'",
    "usb": "lsusb"
  },
  "setting": {
    "server": {
      "get": "uci show tdripcfg | grep tdripcfg | grep -v data",
      "set": "uci set {:s}='{:s}'",
      "commit": "uci commit tdripcfg"
    },
    "network": {
      "wan": {
        "get": "uci show network | grep network.wan. | grep -v network.wan6",
        "set": "uci set {:s}='{:s}'",
        "commit": "uci commit network"
      },
      "lan": {
        "get": "uci show network | grep network.lan. | grep -v 'ip6\\|force_line\\|type\\|force_link\\|proto'",
        "set": "uci set {:s}='{:s}'",
        "commit": "uci commit network"
      }
    }
  },
  "keys": {
    "setting": {
      "server": {
        "upgradeIp": "tdripcfg.tdripcfg.upgradeip",
        "upgradePort": "tdripcfg.tdripcfg.upgradeport",
        "operationIp": "tdripcfg.tdripcfg.operationip",
        "operationPort": "tdripcfg.tdripcfg.operationport",
        "idcardIp": "tdripcfg.tdripcfg.idcardip",
        "idcardPort": "tdripcfg.tdripcfg.idcardport",
        "logsrvUrl": "tdripcfg.tdripcfg.logsrvurl"
      },
      "network": {
        "wan": {
          "name": "network.wan.ifname",
          "proto": "network.wan.proto",
          "ip": "network.wan.ipaddr",
          "mask": "network.wan.netmask",
          "gateway": "network.wan.gateway"
        },
        "lan": {
          "name": "network.lan.ifname",
          "ip": "network.lan.ipaddr",
          "mask": "network.lan.netmask"
        }
      }
    }
  }
}